openapi: 3.0.2
info:
  description: "Claim Details"
  version: "1.0.0"
  title: "Claim Details"
servers:
  - url: "http://localhost:8080"
paths:
  /claimdetails/1.0/:
    get:
      tags:
        - Claim Details
      summary: Get Claim Details by dcn
      operationId: getClaimDetailsByDcn
      parameters:
        - in: header
          name: X-SubmitDateTime
          schema:
            type: string
          required: true
          description: Date/Time from the client in ISO 8601 date format yyyy-MM-dd'T'HH:mm:ss.SSS'Z'
        - in: header
          name: X-ClientSessionId
          schema:
            type: string
          required: false
          description: SessionId of the client
        - in: header
          name: X-UserId
          schema:
            type: string
          required: false
          description: Logged in application user. This will be used for additional
            tracking.
        - in: header
          name: X-UserRole
          schema:
            type: string
          required: false
          description: Role of the logged in application user. This will be used for additional tracking.
        - in: header
          name: X-ClientName
          schema:
            type: string
          required: true
          description: Name of the consumer application
        - in: header
          name: X-TrackingId
          schema:
            type: string
          required: true
          description: This is the unique id used for end to end tracking
        - name: dcn
          in: header
          description: This parameter accepts a single or multiple dcns with comma separated string
          required: true
          schema:
            type: string
      responses:
        200:
          description: Operation Successful
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/ClaimDetailsReply'
        400:
          description: Bad Request
          content:
            application/json:
              schema:
                description: Only Operation Outcome will be sent.
                allOf:
                  - $ref: '#/components/schemas/ClaimDetailsReply'
        401:
          description: Unauthorized
          content:
            application/json:
              schema:
                description: Only Operation Outcome will be sent.
                allOf:
                  - $ref: '#/components/schemas/ClaimDetailsReply'
        403:
          description: Forbidden
          content:
            application/json:
              schema:
                description: Only Operation Outcome will be sent.
                allOf:
                  - $ref: '#/components/schemas/ClaimDetailsReply'
        404:
          description: Not Found
          content:
            application/json:
              schema:
                description: Only Operation Outcome will be sent.
                allOf:
                  - $ref: '#/components/schemas/ClaimDetailsReply'
        500:
          description: Internal Server Error
          content:
            application/json:
              schema:
                description: Only Operation Outcome will be sent.
                allOf:
                  - $ref: '#/components/schemas/ClaimDetailsReply'

